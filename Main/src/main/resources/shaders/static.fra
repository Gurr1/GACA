#version 330

/* Sampler uniforms */
uniform sampler2D diffuseMap;

/* Texture coordinate corresponding to this fragment */
in vec2 texCoords;

in vec3 normals;

/* Fragment output color */
out vec4 COLOR;

void main(){
	// Set fragment output color
	COLOR = vec4(1.0f, 0.0f, 0.0f, 1.0f);
}